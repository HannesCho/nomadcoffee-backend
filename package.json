    "apollo-server": "^2.19.1",
    "apollo-server-express": "^2.19.2",
    "aws-sdk": "^2.839.0",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.8.0",
